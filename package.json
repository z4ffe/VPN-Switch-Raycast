{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "vpn-switch",
  "title": "VPN Switch",
  "description": "Simple VPN Switch",
  "icon": "command-icon.png",
  "author": "zaffe",
  "categories": [
    "System"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "VPN Switch",
      "description": "Simple VPN Switch",
      "mode": "no-view"
    }
  ],
  "preferences": [
    {
      "description": "Which VPN you want to toggle by extension.",
      "label": "VPN Name",
      "name": "vpnName",
      "required": true,
      "title": "VPN Name",
      "type": "textfield",
      "placeholder": "Type VPN Name"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}